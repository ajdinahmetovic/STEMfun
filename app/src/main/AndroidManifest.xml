            </intent-filter>
        </activity>
        <activity android:name=".SignUpAcitvity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".SignInActivity"
            android:screenOrientation="portrait"/>
